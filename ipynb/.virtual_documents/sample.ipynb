from typing import List, Union

def CalcDiffer(data: List) -> List:
    '''
    微分値を求める関数

    Parameters
    ----------
    data : array-like
        波形データ

    Returns
    -------
    differs : array-like
        微分値が格納された配列

    Notes
    -----
    波形データのx軸はインデックスであることが前提。
    '''
    differs = []
    for i in range(0, len(data)-1):
        d = np.abs((data[i+1] - data[i])/1)
        differs.append(d)
    return differs


